---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
import LevelCell from '../components/LevelCell.astro';

import Levels from '../listshit';

for (let i = 0; i < Levels.length; i++) {
	Levels[i]["iter"] = i
}
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header title={SITE_TITLE} />
		<main>
			{Levels.map((levels: any) => (
				<LevelCell title={levels.title} framerate={levels.framerate} creator={levels.creator} ID={levels.ID} verification={levels.verification} iter />
			  ))}
		</main>
		<Footer />
	</body>
</html>
